        </axis>
        <axis name="SIDE">
            <function name="aero/force/SIDE">
                <description>SIDE Force</description>
                <product>
                    <property>aero/qbar-psf</property>
                    <property>metrics/Sw-sqft</property>
                    <sum>
                        <property>aero/coefficients/Csb</property>
