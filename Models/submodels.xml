<PropertyList>
  <!-- MiG-25 submodels config file
       
  0-3: Chaff/flare
  4: droptank
  -->
  
  <!-- 0 -->
  <submodel>
    <name>flaresL</name>
    <model>Aircraft/MiG-25/Models/Effects/Flare/flare.xml</model>
    <trigger>ai/submodels/submodel[0]/flare-release</trigger>
    <speed>400.0</speed>        <!-- ft/sec -->
    <repeat>true</repeat>
    <delay>0.25</delay>
    <count>30</count>
    <offsets>
      <x-m> 3.754</x-m>
      <y-m>-3.322</y-m>
      <z-m> 0.053</z-m>
      <yaw-deg>
	<value>-30</value>
      </yaw-deg>
      <pitch-deg>
	<value>110</value>
      </pitch-deg>
    </offsets>
    <life>5</life>
    <buoyancy>0</buoyancy>
    <weight>2.5</weight>
    <eda>1.0</eda>
    <cd>1.0</cd>
    <no-roll>true</no-roll>
    <wind>true</wind>
  </submodel>

  <!-- 1 -->
  <submodel>
    <name>flaresR</name>
    <model>Aircraft/MiG-25/Models/Effects/Flare/flare.xml</model>
    <trigger>ai/submodels/submodel[0]/flare-release</trigger>
    <speed>400.0</speed>        <!-- ft/sec -->
    <repeat>true</repeat>
    <delay>0.25</delay>
    <count>30</count>
    <offsets>
      <x-m>3.754</x-m>
      <y-m>3.322</y-m>
      <z-m>0.053</z-m>
      <yaw-deg>
	<value>30</value>
      </yaw-deg>
      <pitch-deg>
	<value>110</value>
      </pitch-deg>
    </offsets>
    <yaw-offset>0.0</yaw-offset>
    <pitch-offset>110.0</pitch-offset>
    <life>5</life>
    <buoyancy>0</buoyancy>
    <weight>2.5</weight>
    <eda>1.0</eda>
    <cd>1.0</cd>
    <no-roll>true</no-roll>
    <wind>true</wind>
  </submodel>

  <!-- 2 -->
  <submodel>
    <name>ChaffL</name>
    <model>Aircraft/MiG-25/Models/Effects/Chaff/chaff.xml</model>
    <trigger>ai/submodels/submodel[0]/flare-release</trigger>
    <speed>50</speed>        <!-- ft/sec -->
    <repeat>true</repeat>
    <delay>0.25</delay>
    <count>30</count>
    <offsets>
      <x-m> 3.754</x-m>
      <y-m>-3.322</y-m>
      <z-m> 0.053</z-m>
      <pitch-deg>
	<value>110</value>
      </pitch-deg>
    </offsets>
    <yaw-offset>0</yaw-offset>
    <pitch-offset>110.0</pitch-offset>
    <life>0.2</life>
    <buoyancy>30</buoyancy>
    <weight>0.1</weight>
    <eda>0.001</eda>
    <cd>0.0</cd>
    <no-roll>true</no-roll>
    <wind>true</wind>
    <collision>false</collision>
  </submodel>

  <!-- 3 -->
    <submodel>
    <name>ChaffR</name>
    <model>Aircraft/MiG-25/Models/Effects/Chaff/chaff.xml</model>
    <trigger>ai/submodels/submodel[0]/flare-release</trigger>
    <speed>50</speed>        <!-- ft/sec -->
    <repeat>true</repeat>
    <delay>0.25</delay>
    <count>30</count>
    <offsets>
      <x-m>3.754</x-m>
      <y-m>3.322</y-m>
      <z-m>0.053</z-m>
      <pitch-deg>
	<value>110</value>
      </pitch-deg>
    </offsets>
    <yaw-offset>0</yaw-offset>
    <pitch-offset>110.0</pitch-offset>
    <life>0.2</life>
    <buoyancy>30</buoyancy>
    <weight>0.1</weight>
    <eda>0.001</eda>
    <cd>0.0</cd>
    <no-roll>true</no-roll>
    <wind>true</wind>
    <collision>false</collision>
  </submodel>

  <!-- 4 -->
  <submodel>
    <name>Jettison Fuel Tank</name>
    <model>Aircraft/MiG-25/Models/Stores/Droptank/Droptank.ac</model>
    <trigger>payload/armament/tank-trigger</trigger>
    <trigger-once>true</trigger-once>
    <speed>0.0</speed>
    <count>1</count>
    <offsets>
      <x-m>0.000</x-m>
      <y-m>0.000</y-m>
      <z-m>0.000</z-m>
    </offsets>
    <eda>1.0</eda>
    <weight>250.0</weight>
    <cd>0.3</cd>
    <collision>false</collision>
    <impact>false</impact>
  </submodel>
</PropertyList>
