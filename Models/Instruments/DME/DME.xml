<!--Blanker-->
<animation>
  <type>rotate</type>
  <object-name>blanker</object-name>
  <property>instrumentation/idr-1[0]/caged-flag</property>
  <offset-deg>-90.0</offset-deg>
  <factor>90.0</factor>
  <center>
    <x-m>-0.02264</x-m>
