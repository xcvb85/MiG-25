<PropertyList>
 <path>idr-1.ac</path>

  <animation>
    <type>noshadow</type>
    <object-name>glass</object-name>
  </animation>
  
  <animation>
    <type>shader</type>
    <shader>chrome</shader>
    <texture>Aircraft/MiG-25/Models/Effects/glass_shader.png</texture>
    <object-name>glass</object-name>
  </animation>

<animation>
  <type>material</type>
  <object-name>blanker</object-name>
    <emission>
      <factor-prop>instrument-lights</factor-prop>
      <red>   1.0 </red>
      <green> 0.0 </green>
      <blue>  0.0 </blue>
    </emission>
  </animation>

<!--Digit wheels-->
<animation>
  <type>rotate</type>
  <object-name>dig_hm</object-name>
  <property>instrumentation/idr-1[0]/indicated-wheels_dec_m</property>
  <factor>36.0</factor>
  <center>
    <x-m>-0.02264</x-m>
    <y-m>0.0</y-m>
    <z-m>0.0</z-m>
  </center>
  <axis>
    <x>0.0</x>
    <y>-1.0</y>
    <z>0.0</z>
  </axis>
</animation>

<animation>
  <type>rotate</type>
  <object-name>dig_ones</object-name>
  <property>instrumentation/idr-1[0]/indicated-wheels_hund_m</property>
  <interpolation>
    <entry><ind>	0.0</ind>	<dep>	0.0</dep></entry>
    <entry><ind>	0.9</ind>	<dep>	0.1</dep></entry>
    <entry><ind>	1.0</ind>	<dep>	36.0</dep></entry>
    <entry><ind>	1.9</ind>	<dep>	36.1</dep></entry>
    <entry><ind>	2.0</ind>	<dep>	72.0</dep></entry>
    <entry><ind>	2.9</ind>	<dep>	72.1</dep></entry>
    <entry><ind>	3.0</ind>	<dep>	108.0</dep></entry>
    <entry><ind>	3.9</ind>	<dep>	108.1</dep></entry>
    <entry><ind>	4.0</ind>	<dep>	144.0</dep></entry>
    <entry><ind>	4.9</ind>	<dep>	144.1</dep></entry>
    <entry><ind>	5.0</ind>	<dep>	180.0</dep></entry>
    <entry><ind>	5.9</ind>	<dep>	180.1</dep></entry>
    <entry><ind>	6.0</ind>	<dep>	216.0</dep></entry>
    <entry><ind>	6.9</ind>	<dep>	216.1</dep></entry>
    <entry><ind>	7.0</ind>	<dep>	252.0</dep></entry>
    <entry><ind>	7.9</ind>	<dep>	252.1</dep></entry>
    <entry><ind>	8.0</ind>	<dep>	288.0</dep></entry>
    <entry><ind>	8.9</ind>	<dep>	288.1</dep></entry>
    <entry><ind>	9.0</ind>	<dep>	324.0</dep></entry>
    <entry><ind>	9.9</ind>	<dep>	324.1</dep></entry>
    <entry><ind>	9.99</ind>	<dep>	359.9</dep></entry>
  </interpolation>
  <center>
    <x-m>-0.02264</x-m>
    <y-m>0.0</y-m>
    <z-m>0.0</z-m>
  </center>
  <axis>
    <x>0.0</x>
    <y>-1.0</y>
    <z>0.0</z>
  </axis>
</animation>

